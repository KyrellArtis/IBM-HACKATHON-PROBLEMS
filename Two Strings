#include<iostream>

using namespace std;


int main(){
  bool a[256];
  int T;
  cin>>T;
  string x,y;
  while(T>0){
    T -- ;
    cin>>x;
    cin>>y;
    for (int i=0;i<256;i++) a[i]=false;
    for (int i=0;i<x.length(); i++) {
      a[x[i]] = true;
    }
    
    bool sub = false;
    for (int i=0;i<y.length();i++) {
      if (a[y[i]]) sub=true;
    }
    
    if (sub) cout<<"YES\n";
    else cout<<"NO\n";
  }
  
  
}
