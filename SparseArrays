#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>

using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int s;
    int n;
    int dups;
    int strings;
    vector <string> stringArr;
    string in;
    
    cin >> s;
    for(int count = 0; count < s; count++)  {
        cin >> in;
        stringArr.push_back(in);
    }
    
    cin >> strings;
    for(int i = 0; i < strings; i++)  {
        cin >> in;
        for(int count = 0; count < stringArr.size(); count++)  {
            if(in == stringArr[count])  {
                dups++;
            }
        }
        cout << dups << endl;
        dups = 0;
    }
    return 0;
}
